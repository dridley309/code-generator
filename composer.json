{
    "name": "dridley309/code-generator",
    "description": "Generate secure codes using customisable rule criteria",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dridley309\\CodeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Dridley309\\CodeGenerator\\Tests\\": "tests/"
        }
      },
    "authors": [
        {
            "name": "Dominic Ridley",
            "email": "domm.rid@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Dridley309\\CodeGenerator\\CodeGeneratorServiceProvider"
            ]
        }
    }
  
}
